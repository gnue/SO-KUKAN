		<link>/gnue/2004/12/31/atm-%e3%81%ae%e7%8f%be%e9%87%91%e5%bc%95%e5%87%ba%e3%81%97/</link>
					<comments>/gnue/2004/12/31/atm-%e3%81%ae%e7%8f%be%e9%87%91%e5%bc%95%e5%87%ba%e3%81%97/#comments</comments>
		
